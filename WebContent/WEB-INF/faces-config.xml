<?xml version="1.0" encoding="UTF-8"?>
<faces-config

    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">



<navigation-rule>
	<from-view-id>/index.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>crear</from-outcome>
		<to-view-id>/altaAdmin.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
		<from-outcome>eliminar</from-outcome>
		<to-view-id>/eliminarAdmin.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>	


<navigation-rule>
<from-view-id>/altaAdmin.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>succes</from-outcome>
		<to-view-id>/exito.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>


<navigation-rule>
<from-view-id>/eliminarAdmin.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>succes</from-outcome>
		<to-view-id>/exito.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>

</faces-config>
